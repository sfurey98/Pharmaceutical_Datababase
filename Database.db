drop table IF EXISTS store;
drop table IF EXISTS manager;
drop table IF EXISTS worker;
drop table IF EXISTS customer;
drop table IF EXISTS medicine;
drop table IF EXISTS sales;
drop table IF EXISTS purchases;
drop table IF EXISTS inventory;
Drop table IF EXISTS manufacturer;

CREATE TABLE store(store_name char(30) NOT NULL, manager_num int, address char(30), phone char(15), PRIMARY KEY(store_name));

CREATE TABLE manager(manager_num int NOT NULL, manager_name char(10), employee_num int, PRIMARY KEY(manager_num), FOREIGN KEY (employee_num) REFERENCES worker(employee_num)) ENGINE =INNODB;

CREATE TABLE worker(employee_num int NOT NULL, manager_num int, employee_name char(10), position char(15), PRIMARY KEY(employee_num), FOREIGN KEY (manager_num) REFERENCES manager(manager_num)) ENGINE =INNODB;

CREATE TABLE customer(customer_num int NOT NULL, customer_name char(10), address char(30), PRIMARY KEY(customer_num)) ENGINE =INNODB;

CREATE TABLE medicine(ndc_num int NOT NULL, manufacturer_name char(20) NOT NULL, type char(30), name char(15), inventory_num int, PRIMARY KEY(ndc_num), FOREIGN KEY (manufacturer_name) REFERENCES manufacturer(manufacturer_name), FOREIGN KEY (inventory_num) REFERENCES inventory(inventory_num)) ENGINE =INNODB;

CREATE TABLE manufacturer(manufacturer_name char(20) NOT NULL, PRIMARY KEY (manufacturer_name)) ENGINE =INNODB;

CREATE TABLE sales(sale_num int NOT NULL, employee_num int, customer_num int, inventory_num int, quantity int, PRIMARY KEY(sale_num), FOREIGN KEY (employee_num) REFERENCES worker(employee_num), FOREIGN KEY (customer_num) REFERENCES customer(customer_num), FOREIGN KEY (inventory_num) REFERENCES inventory(inventory_num)) ENGINE =INNODB;

CREATE TABLE purchases (order_num int NOT NULL, manufacturer_name char(20), inventory_num int, quantity int, PRIMARY KEY(order_num), FOREIGN KEY (inventory_num) REFERENCES inventory(inventory_num), FOREIGN KEY (manufacturer_name) REFERENCES manufacturer(manufacturer_name)) ENGINE =INNODB;

CREATE TABLE inventory(inventory_num int NOT NULL, quantity int NOT NULL, ndc_num int, PRIMARY KEY(inventory_num), FOREIGN KEY (ndc_num) REFERENCES medicine(ndc_num)) ENGINE =INNODB;

